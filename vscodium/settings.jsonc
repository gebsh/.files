rrc.yaml, .puppeteerrc.js, .puppeteerrc.cjs, puppeteer.config.js, puppeteer.config.cjs, rollup.config.js, rollup.config.cjs, rollup.config.mjs, rollup.config.ts, rollup.config.cts, rollup.config.mts, rspack.config.js, rspack.config.cjs, rspack.config.mjs, rspack.config.ts, rspack.config.cts, rspack.config.mts, stylelint.config.js, stylelint.config.cjs, stylelint.config.mjs, tsconfig.json, tsconfig.*.json, *.tsbuildinfo, webpack.config.js, webpack.config.cjs, webpack.config.mjs, webpack.config.ts, webpack.config.cts, webpack.config.mts, .yarnrc, yarn.lock",
		"pyproject.toml": ".python-version, .ruff.toml, ruff.toml, uv.lock, uv.toml",
		"README*": "ARCHITECTURE*, AUTHORS*, CHANGELOG*, CODE_OF_CONDUCT*, CONTRIBUTING*, CONTRIBUTORS*, LICENSE*, README*, SECURITY*"
	},
	"explorer.incrementalNaming": "smart",
	"explorer.sortOrderLexicographicOptions": "upper",
	"extensions.autoUpdate": false,
	"extensions.experimental.deferredStartupFinishedActivation": true,
	"extensions.ignoreRecommendations": true,
	"files.associations": {
		".clang-format": "yaml",
		"*.rels": "xml",
		"LICENSE": "plaintext",
		"TAG_EDITMSG": "git-commit"
	},
	"files.defaultLanguage": "plaintext",
	"files.eol": "\n",
	"files.insertFinalNewline": true,
	"files.readonlyFromPermissions": true,
	"files.readonlyInclude": {
		"**/.venv/**": true,
		"**/build/**": true,
		"**/dist/**": true,
		"**/node_modules/**": true,
		"**/target/**": true
	},
	"files.trimFinalNewlines": true,
	"files.trimTrailingWhitespace": true,
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/.hg/store/**": true,
		"**/.venv/**": true,
		"**/build/**": true,
		"**/dist/**": true,
		"**/node_modules/**": true,
		"**/target/**": true
	},
	"notebook.codeActionsOnSave": {
		"notebook.source.fixAll": "explicit",
		"notebook.source.organizeImports": "explicit"
	},
	"notebook.formatOnSave.enabled": true,
	"scm.diffDecorations": "gutter",
	"scm.diffDecorationsGutterPattern": {
		"added": true,
		"modified": true
	},
	"scm.diffDecorationsGutterWidth": 2,
	"scm.diffDecorationsIgnoreTrimWhitespace": "inherit",
	"scm.inputFontFamily": "'MonoLisa Preconfigured Ligatures Nerd Font', 'Iosevka Fixed Extended', 'SF Mono', Menlo, Monaco, 'DejaVu Sans Mono', 'Cascadia Code', monospace",
	"scm.inputFontSize": 10.5,
	"scm.repositories.sortOrder": "name",
	"search.exclude": {
		"**/.venv": true,
		"**/build": true,
		"**/dist": true,
		"**/node_modules": true,
		"**/target": true
	},
	"search.showLineNumbers": true,
	"search.smartCase": true,
	"telemetry.feedback.enabled": false,
	"terminal.external.osxExec": "ghostty",
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.cursorStyleInactive": "none",
	"terminal.integrated.defaultProfile.osx": "Nushell",
	"terminal.integrated.drawBoldTextInBrightColors": false,
	"terminal.integrated.fontFamily": "'MonoLisa Nerd Font', 'Iosevka Fixed Extended', 'SF Mono', Menlo, Monaco, 'DejaVu Sans Mono', 'Cascadia Code', monospace",
	"terminal.integrated.fontLigatures.enabled": true,
	"terminal.integrated.fontLigatures.featureSettings": "'liga' off, 'zero', 'ss01', 'ss03', 'ss07', 'ss08', 'ss12', 'ss13', 'ss15', 'ss16'",
	"terminal.integrated.fontSize": 13,
	"terminal.integrated.fontWeight": "300",
	"terminal.integrated.hideOnStartup": "always",
	"terminal.integrated.lineHeight": 1.2,
	"terminal.integrated.profiles.osx": {
		"bash": null,
		"zsh": null
	},
	"terminal.integrated.rightClickBehavior": "default",
	"terminal.integrated.smoothScrolling": true,
	"update.showReleaseNotes": false,
	"window.newWindowDimensions": "inherit",
	"window.newWindowProfile": "Default",
	"window.title": "${activeEditorMedium}${separator}${rootName}",
	"workbench.activityBar.location": "bottom",
	"workbench.cloudChanges.autoResume": "off",
	"workbench.cloudChanges.continueOn": "off",
	"workbench.colorTheme": "Catppuccin Mocha",
	"workbench.editor.defaultBinaryEditor": "hexEditor.hexedit",
	"workbench.editor.historyBasedLanguageDetection": false,
	"workbench.editor.limit.enabled": true,
	"workbench.editor.limit.excludeDirty": true,
	"workbench.editor.limit.perEditorGroup": true,
	"workbench.editor.limit.value": 6,
	"workbench.editor.pinnedTabSizing": "compact",
	"workbench.editor.untitled.labelFormat": "name",
	"workbench.iconTheme": "catppuccin-mocha",
	"workbench.layoutControl.type": "toggles",
	"workbench.list.smoothScrolling": true,
	"workbench.localHistory.exclude": {
		"**/.git/**": true,
		"**/.hg/**": true,
		"**/.venv/**": true,
		"**/build/**": true,
		"**/dist/**": true,
		"**/node_modules/**": true,
		"**/target/**": true
	},
	"workbench.preferredDarkColorTheme": "Catppuccin Mocha",
	"workbench.preferredLightColorTheme": "Catppuccin Latte",
	"workbench.secondarySideBar.defaultVisibility": "hidden",
	"workbench.sideBar.location": "right",
	"workbench.startupEditor": "none",
	"workbench.tree.indent": 16,
	"workbench.tree.renderIndentGuides": "none",
	"workbench.tree.stickyScrollMaxItemCount": 3,
	"workbench.welcomePage.extraAnnouncements": false,

	// Extension settings
	"catppuccin-icons.associations.extensions": {
		"gitattributes": "git",
		"gitconfig": "git",
		"rels": "xml"
	},
	"catppuccin.boldKeywords": false,
	"catppuccin.extraBordersEnabled": true,
	"catppuccin.italicComments": false,
	"catppuccin.italicKeywords": false,
	"catppuccin.workbenchMode": "minimal",
	"css.format.enable": false,
	"debugpy.showPythonInlineValues": true,
	"editorconfig.showMenuEntry": false,
	"errorLens.borderRadius": "0.34em",
	"errorLens.enabledDiagnosticLevels": ["warning", "error"],
	"errorLens.margin": "1ch",
	"errorLens.messageBackgroundMode": "message",
	"errorLens.messageMaxChars": 160,
	"errorLens.messageTemplate": "[$source] $message",
	"errorLens.padding": "0.1em 0.3em",
	"errorLens.replaceLinebreaksSymbol": "â†©",
	"errorLens.severityText": [" Err: ", "Warn: ", "Info: ", "Hint: "],
	"evenBetterToml.formatter.allowedBlankLines": 1,
	"evenBetterToml.formatter.columnWidth": 100,
	"evenBetterToml.semanticTokens": true,
	"git.branchProtection": ["main", "master"],
	"git.enableCommitSigning": true,
	"git.inputValidationSubjectLength": null,
	"git.openRepositoryInParentFolders": "never",
	"github.gitProtocol": "ssh",
	"html.autoCreateQuotes": false,
	"html.format.enable": false,
	"javascript.format.enable": false,
	"javascript.inlayHints.functionLikeReturnTypes.enabled": true,
	"javascript.inlayHints.parameterNames.enabled": "all",
	"javascript.inlayHints.parameterTypes.enabled": true,
	"javascript.inlayHints.propertyDeclarationTypes.enabled": true,
	"javascript.inlayHints.variableTypes.enabled": true,
	"javascript.preferences.importModuleSpecifierEnding": "js",
	"javascript.preferences.quoteStyle": "single",
	"javascript.preferences.useAliasesForRenames": false,
	"js/ts.implicitProjectConfig.target": "ESNext",
	"json.format.enable": false,
	"less.format.enable": false,
	"markdown.validate.enabled": true,
	"npm.fetchOnlinePackageInfo": false,
	"python.experiments.enabled": false,
	"python.languageServer": "None",
	"python.terminal.activateEnvironment": false,
	"python.terminal.shellIntegration.enabled": false,
	"rust-analyzer.assist.emitMustUse": true,
	"rust-analyzer.assist.preferSelf": true,
	"rust-analyzer.check.command": "clippy",
	"rust-analyzer.hover.actions.enable": false,
	"rust-analyzer.hover.memoryLayout.alignment": "both",
	"rust-analyzer.hover.memoryLayout.offset": "both",
	"rust-analyzer.imports.granularity.enforce": true,
	"rust-analyzer.imports.group.enable": false,
	"rust-analyzer.inlayHints.bindingModeHints.enable": true,
	"rust-analyzer.inlayHints.closureCaptureHints.enable": true,
	"rust-analyzer.inlayHints.expressionAdjustmentHints.enable": "always",
	"rust-analyzer.inlayHints.lifetimeElisionHints.enable": "skip_trivial",
	"rust-analyzer.notifications.cargoTomlNotFound": false,
	"scss.format.enable": false,
	"ty.experimental.autoImport": true,
	"ty.experimental.rename": true,
	"typescript.disableAutomaticTypeAcquisition": true,
	"typescript.format.enable": false,
	"typescript.inlayHints.functionLikeReturnTypes.enabled": true,
	"typescript.inlayHints.parameterNames.enabled": "all",
	"typescript.inlayHints.parameterTypes.enabled": true,
	"typescript.inlayHints.propertyDeclarationTypes.enabled": true,
	"typescript.inlayHints.variableTypes.enabled": true,
	"typescript.preferences.importModuleSpecifierEnding": "js",
	"typescript.preferences.preferTypeOnlyAutoImports": true,
	"typescript.preferences.quoteStyle": "single",
	"typescript.preferences.useAliasesForRenames": false,

	// Language-specific settings
	"[c]": {
		// Follow the already established popular code style. Usage of tabs in C/C++ might be
		// annoying due to how often formatters align things there. (And alignment is better done
		// with spaces.)
		"editor.indentSize": 2,
		"editor.insertSpaces": true
	},
	"[cpp]": {
		"editor.indentSize": 2,
		"editor.insertSpaces": true
	},
	"[css]": {
		"editor.codeActionsOnSave": {
			"source.fixAll.biome": "explicit",
			"source.organizeImports.biome": "explicit"
		},
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[git-commit]": {
		"editor.rulers": [72]
	},
	"[html]": {
		"editor.codeActionsOnSave": {
			"source.fixAll.biome": "explicit",
			"source.organizeImports.biome": "explicit"
		},
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[javascript]": {
		"editor.codeActionsOnSave": {
			"source.fixAll.biome": "explicit",
			"source.organizeImports.biome": "explicit"
		},
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[json]": {
		"editor.codeActionsOnSave": {
			"source.fixAll.biome": "explicit"
		},
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[jsonc]": {
		"editor.codeActionsOnSave": {
			"source.fixAll.biome": "explicit"
		},
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[log]": {
		"editor.wordWrap": "off"
	},
	"[markdown]": {
		"editor.insertSpaces": true, // Use spaces for consistency with wrapped content's alignment.
		"editor.quickSuggestions": {
			"comments": "off",
			"other": "off",
			"strings": "off"
		},
		"editor.unicodeHighlight.ambiguousCharacters": false,
		"editor.wordWrap": "bounded"
	},
	"[plaintext]": {
		"editor.quickSuggestions": {
			"comments": "off",
			"other": "off",
			"strings": "off"
		},
		"editor.unicodeHighlight.ambiguousCharacters": false,
		"editor.unicodeHighlight.invisibleCharacters": false,
		"editor.wordWrap": "bounded"
	},
	"[python]": {
		"editor.codeActionsOnSave": {
			"source.fixAll": "explicit",
			"source.organizeImports": "explicit"
		},
		"editor.defaultFormatter": "charliermarsh.ruff"
	},
	"[rust]": {
		"editor.defaultFormatter": "rust-lang.rust-analyzer"
	},
	"[toml]": {
		"editor.defaultFormatter": "tamasfe.even-better-toml"
	},
	"[typescript]": {
		"editor.codeActionsOnSave": {
			"source.fixAll.biome": "explicit",
			"source.organizeImports.biome": "explicit"
		},
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[yaml]": {
		"editor.indentSize": 2, // Indentation greater than 2 is annoying to work with in YAML.
		"editor.insertSpaces": true // YAML doesn't support tab indentation.
	}
}
