		"*.cpp": "${capture}.hpp, ${capture}.h",
		"*.css": "${capture}.css.map",
		"*.cts": "${capture}.cjs",
		"*.js": "${capture}.js.map, ${capture}.d.ts, ${capture}.d.ts.map",
		"*.mjs": "${capture}.mjs.map, ${capture}.d.mts, ${capture}.d.mts.map",
		"*.mts": "${capture}.mjs",
		"*.py": "${capture}.pyi",
		"*.ts": "${capture}.js",
		"*.wat": "${capture}.wasm",
		"Cargo.toml": "Cargo.lock, .clippy.toml, clippy.toml, .rustfmt.toml, rustfmt.toml, rust-toolchain.toml",
		"Dockerfile": ".dockerignore",
